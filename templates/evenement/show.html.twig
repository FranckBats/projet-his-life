    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ evenement.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ evenement.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
